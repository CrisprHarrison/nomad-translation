{
    "upload": {
        "files": [
            {
                "path": "locales",
                "pattern": "locales/English.strings",
                "type": "ios-strings"
            },
            {
                "path": "description",
                "pattern": "description/English.txt",
                "type": "text"
            }
        ],

        "deprecate": "file",
        "filterSource": false,
        "forceCurrent": true
    },

    "download": {
        "files": "${path}/${langNameEn}.${fileExt}"
    }
}